{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "waitFor": "onCreateCommand",
  "updateContentCommand": "npm install",
  "postCreateCommand": "",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "src/App.jsx"
      ]
    },
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode"
      ]
    }
  },
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [3000]
}
